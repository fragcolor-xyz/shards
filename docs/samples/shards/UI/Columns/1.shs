GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel({
        UI.Columns(
          Contents: [
            { "Column 1" | UI.Label }
            { "Column 2" | UI.Label }
          ]
        )
      })
    )

    GFX.Render(Steps: render-steps)
  }
)