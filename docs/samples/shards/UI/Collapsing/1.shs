GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel({
        UI.Collapsing(
          Heading: "Heading"
          DefaultOpen: true
          Contents: {
            "Content" | UI.Label
          }
        )
      })
    )

    GFX.Render(Steps: render-steps)
  }
)