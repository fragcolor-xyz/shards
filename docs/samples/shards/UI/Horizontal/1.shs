GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel(
        UI.Horizontal({
          "Hello" | UI.Label
          UI.Separator
          "World" | UI.Label
        })
      )
    )

    GFX.Render(Steps: render-steps)
  }
)