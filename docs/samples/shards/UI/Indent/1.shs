GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel({
        "Part A" | UI.Label
        UI.Indent(
          Contents: {
            "1." | UI.Label
            UI.Indent(
              Contents: {
                "1.1." | UI.Label
                "1.2." | UI.Label
              }
            )
            "2." | UI.Label
          }
        )
        UI.Separator
        "Part B" | UI.Label
      })
    )

    GFX.Render(Steps: render-steps)
  }
)