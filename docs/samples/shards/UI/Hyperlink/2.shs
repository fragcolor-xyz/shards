GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel({
        "https://docs.fragnova.com" | 
        UI.Hyperlink(
          Label: "Fragnova Documentation"
          Style: {
            italics: true
          }
        )
      })
    )

    GFX.Render(Steps: render-steps)
  }
)