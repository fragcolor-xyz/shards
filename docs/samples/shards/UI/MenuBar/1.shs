GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.TopPanel(
        Contents: 
          UI.MenuBar(
            Contents: 
              UI.Menu("Menu" {
                "Some Text" | UI.Label
              })
          )
      )     
    )

    GFX.Render(Steps: render-steps)
  }
)