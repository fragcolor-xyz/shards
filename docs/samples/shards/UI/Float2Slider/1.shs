GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel({
        UI.Float2Slider(
          Variable: float2
          Min: @f2(0)
          Max: @f2(1)
        ) | ExpectFloat2
      })
    )

    GFX.Render(Steps: render-steps)
  }
)