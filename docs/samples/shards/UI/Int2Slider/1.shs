GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel({
        UI.Int2Slider(
          Variable: int2
          Min: @i2(1)
          Max: @i2(100)
        ) | ExpectInt2
      })
    )

    GFX.Render(Steps: render-steps)
  }
)