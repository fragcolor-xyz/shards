GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel({
        [1 2 3 4 5 6 7 8 9]
        | UI.ListBox(
          Index: index
          Template: {
            ToString | UI.Label
          }
        )
        | ExpectInt >= value

        UI.Horizontal({
          "Selected index: " | UI.Label
          index | ToString | UI.Label
        })
      })
    )

    GFX.Render(Steps: render-steps)
  }
)