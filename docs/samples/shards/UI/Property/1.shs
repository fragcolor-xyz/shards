GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
        ui-draw-queue
        UI.CentralPanel(
          Contents: {
            UI.Property(UIProperty::RemainingSpace)
            Once({
              | Log
            })
          }
        )
    )

    GFX.Render(Steps: render-steps)
  }
)