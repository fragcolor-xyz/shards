GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel({
        UI.Checkbox(
          Label: "Checked"
          Style: {
            color: @color(255 0 255)
            italics: true
          }
          Variable: checked
        )
      })
    )

    GFX.Render(Steps: render-steps)
  }
)