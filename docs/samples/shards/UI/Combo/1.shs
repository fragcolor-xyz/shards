GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel({
        ["α Α" "ω Ω"] | UI.Combo(Label: "Greek letter" Index: index)
        | ExpectString >= letter
      })
    )

    GFX.Render(Steps: render-steps)
  }
)