GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
      ui-draw-queue
      UI.CentralPanel({
        UI.Disable(Disable: true Contents: {
          UI.Button("Can't be clicked")
        })
      })
    )

    GFX.Render(Steps: render-steps)
  }
)