GFX.MainWindow(
  Contents: {
    Once({
      GFX.DrawQueue >= ui-draw-queue
      GFX.UIPass(ui-draw-queue) >> render-steps
    })
    UI(
        ui-draw-queue
        UI.DockArea({
          UI.Tab(Title: "Tab 1" Contents: {
            "Tab 1 contents" | UI.Label
          })
          UI.Tab(Title: "Tab 2" Contents: {
            "Tab 2 contents" | UI.Label
          })
        })
    )

    GFX.Render(Steps: render-steps)
  }
)